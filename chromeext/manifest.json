{
	"manifest_version": 2,
	"name": "QR...",
	"version": "1.0.3",
	"author": "RastaCre",
	"default_locale": "en",
	"description": "Add a context menu item to convert selected object into QR.",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png"
	},
	"permissions": [ "contextMenus", "tabs" ],
	"background": {
		"scripts": ["script.js"]
	},
	"options_page": "options.html",
	"minimum_chrome_version": "6"
}